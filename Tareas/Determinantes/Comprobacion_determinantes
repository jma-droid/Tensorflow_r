
import tensorflow as tf

##Se establecen las matrices con sus respectivos elementos
A = tf.constant([[7 , -3 , 5],
                 [2 , 1 , 8],
                 [-3 , -6 , 9]], dtype=tf.float32 ) 

B = tf.constant([[1 , 2 , 3],
                 [4 , 5 , -1],
                 [3 , -2 , 6]], dtype=tf.float32 )

C = tf.constant([[2 , 1 , 0],
                 [3 , 5 , 4],
                 [7 , 0 , -2]], dtype=tf.float32)

#Se establecen las variables detx y se iguala su valor al resultado del comando tf.linalg.det(X)
#el cual calcula el valor del determinante de una matriz
deta = tf.linalg.det(A)
detb = tf.linalg.det(B)
detc = tf.linalg.det(C)

#Se imprimen los valores de los determinantes de las matrices especificando cual determinante es 
#de x matriz
print("Determinante de A: ",deta)
print("Determinante de B: ",detb)
print("Determinante de C: ",detc)
